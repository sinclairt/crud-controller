{
  "name": "sinclair/crud-controller",
  "description": "Stop repeating yourself in controllers",
  "type": "library",
  "require": {
    "illuminate/view": "^5.1",
    "illuminate/support": "^5.1",
    "illuminate/contracts": "^5.1",
    "sinclair/magic-views": "1.*",
    "sinclair/responses": "1.*",
    "sinclair/repository": "1.*"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0",
    "phpspec/phpspec": "~2.1",
    "laracasts/testdummy": "~2.0",
    "laravel/laravel": "dev-develop"
  },
  "autoload": {
    "psr-0": {
      "Sinclair\\CrudController": "src/"
    }
  },
  "minimum-stability": "dev",
  "scripts": {
    "phpunit": "./vendor/bin/phpunit;"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://satis.ominor.co.uk"
    }
  ],
  "config": {
    "preferred-install": "dist",
  }
}
